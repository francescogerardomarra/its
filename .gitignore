# Operating System files
.DS_Store        # macOS
Thumbs.db        # Windows

# IDEs and text editors
.vscode/          # VS Code settings
.idea/            # JetBrains IDEs
*.sublime-workspace
*.sublime-project

# Node.js
node_modules/     # Node.js modules directory

# Python
__pycache__/      # Python bytecode
*.py[cod]         # Python compiled files
*.pyo              # Python optimized files
*.pyd              # Python extension modules

# Logs
*.log

# Build output directories
/dist
/build

# Dependencies
*.tgz
*.tar.gz

# Test coverage
coverage/

# Environment files
.env              # Environment variable files (DO NOT commit this)
.env.local        # Local env files

# npm & yarn lockfiles (if you use Yarn, include both)
package-lock.json
yarn.lock

# Compiled files
*.class           # Java class files
*.exe             # Executables
*.dll             # Dynamic link library

# OS generated files
ehthumbs.db
Desktop.ini

# Other common files to ignore
*.bak
*.swp
*.swo
